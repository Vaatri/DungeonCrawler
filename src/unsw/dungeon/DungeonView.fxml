<BorderPane fx:id="frame" onKeyPressed="#handleKeyPress" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <GridPane fx:id="squares" onKeyPressed="#handleKeyPress" prefHeight="355.0" prefWidth="353.0">
